{
  "cli": {
    "version": ">= 14.7.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_SENTRY_DSN": "@EXPO_PUBLIC_SENTRY_DSN",
        "EXPO_PUBLIC_GMAPS_API_KEY": "@EXPO_PUBLIC_GMAPS_API_KEY"
      }
    },
    "production": {
      "env": {
        "EXPO_PUBLIC_SENTRY_DSN": "@EXPO_PUBLIC_SENTRY_DSN",
        "EXPO_PUBLIC_GMAPS_API_KEY": "@EXPO_PUBLIC_GMAPS_API_KEY"
      }
    }
  }
}